{
    "root": true,
    "env": {
        "node": true
    },
    "parser": "@babel/eslint-parser",
    "extends": [
        "eslint:recommended", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 8,
        "requireConfigFile": false
    },
    "rules": {
        "no-console": "off",
        "no-debugger": "off",
        "prettier/prettier": ["error"]
    }
}
