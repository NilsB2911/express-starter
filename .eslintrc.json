{
    "root": true,
    "env": {
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended"],
    "plugins": ["prettier", "@typescript-eslint"],
    "parserOptions": {
        "ecmaVersion": 8,
        "requireConfigFile": false
    },
    "rules": {
        "no-console": "off",
        "no-debugger": "off",
        "prettier/prettier": ["error"]
    }
}
